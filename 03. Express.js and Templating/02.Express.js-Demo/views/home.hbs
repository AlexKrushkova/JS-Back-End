<ul>
    {{#each users}}
    <li><a href="/user{{}}">{{name}} - {{age}}</a></li>
    {{/each}}
</ul>

<form action="/user"method='post'>
<div>
      <label for="name">Name</label>
      <input type="name" type ="text" value= "{{selectedUser.name}}">
</div>
<div>
      <label for="age">Age:</label>
      <input type="age" type="number" value= "{{selectedUser.age}}">
</div>
<button>Save</button>
</form>